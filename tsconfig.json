{
"compilerOptions": {
    "target": "ES2020",
    "module": "ES6",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
},
"include": [
    "src/**/*.ts"
,
"src/server.js"
],
"exclude": [
    "node_modules"
]
}
